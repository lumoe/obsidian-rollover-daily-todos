{
  "name": "obsidian-rollover-daily-todos",
  "version": "1.1.3",
  "description": "This Obsidian.md plugin rolls over incomplete TODOs from the previous daily note to today's daily note. (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs watch",
    "prebuild": "pnpm i && pnpm clean",
    "clean": "rimraf main.js data.json",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "build:dev": "node esbuild.config.mjs",
    "test": "vitest",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "Matthew Sessions",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/state": "^6.0.0",
    "@codemirror/view": "^6.0.0",
    "builtin-modules": "^3.3.0",
    "esbuild": "0.14.47",
    "obsidian": "latest",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.5",
    "tslib": "^2.5.0",
    "vite": "^4.0.3",
    "vitest": "^0.25.7"
  }
}
